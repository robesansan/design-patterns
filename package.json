{
  "name": "designpatterns",
  "version": "1.0.0",
  "description": "just doing a review of some design patterns I can use in the frontend part",
  "main": "index.js",
  "directories": {
    "lib": "lib"
  },
  "scripts": {
    "test": "echo \"Error: no test specified\" && exit 1",
    "build": "tsc -p .",
    "watch": "tsc -w -p .",
    "format": "prettier --write src/**/*.ts",
    "iterator": "npx ts-node ./src/behavioral/iterator/index.ts",
    "iterator-symbol": "npx ts-node ./src/behavioral/iterator/symbol.ts",
    "strategy": "npx ts-node ./src/behavioral/strategy/index.ts",
    "abstract-factory": "npx ts-node ./src/creational/abstract-factory/index.ts",
    "factory-method": "npx ts-node ./src/creational/factory-method/index.ts",
    "decorator": "npx ts-node ./src/structural/decorator/index.ts",
    "facade": "npx ts-node ./src/structural/facade/index.ts",
    "proxy": "npx ts-node ./src/structural/proxy/index.ts"
  },
  "repository": {
    "type": "git",
    "url": "git+https://github.com/robesansan/designpatterns.git"
  },
